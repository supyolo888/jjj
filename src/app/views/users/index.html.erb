<% provide(:title, 'All users') %>
<h1>All users</h1>

<div class="row">
  <div class="search_form">
    <%= render 'users/search_form' %>
  </div>
</div>

<%= will_paginate %>

<% unless @users.empty? %>
  <ul class="users">
    <%= render @users %>
  </ul>
<% else %>
  <p class="search_message">
    Couldn't find any user.
  </p>
<% end %>

<%= will_paginate %>